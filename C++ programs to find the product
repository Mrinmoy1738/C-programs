#include<iostream>

using namespace std;

int main()
{
    
    float num1, num2, result;

    cout << "Enter first Number : ";
    cin >> num1;

    cout << "\nEnter second Number : ";
    cin >> num2;

  result = num1 * num2;  
  cout << "\nproduct is : " << result;

    return 0;
}
  Output:
  Enter first nunber: 3.4
  Enter second number: 5.5
  Product: 18.7
